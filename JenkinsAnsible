node{
   stage('SCM Checkout'){
       git credentialsId: 'git-creds', url: 'https://github.com/deva9017/Vedika-Service.git'
   }
   stage('gradle Package'){
     sh "gradle clean build"
     }
     stage('playbook exc'){
  sh label: '', script: '''cd /home/ubuntu
     sudo ansible-playbook scriptn.yml'''
}
}
